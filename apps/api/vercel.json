{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "buildCommand": "cd ../.. && pnpm turbo build --filter=api",
  "devCommand": "pnpm dev",
  "installCommand": "pnpm install"
}
